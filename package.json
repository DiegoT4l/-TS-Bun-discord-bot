{
  "name": "base-discord-bot-ts",
  "version": "1.0.0",
  "description": "A Discord bot written in TypeScript",
  "type": "module",
  "module": "dist/index.js",
  "scripts": {
    "start": "bun run .",
    "dev": "bun run tsx watch src/index.ts && bun run build && bun run start",
    "build": "bunx tsc",
    "clean": "rm -rf dist && echo 'dist folder killed.'"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "bun.js"
  ],
  "author": "DiegoT4l",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@types/bun": "latest",
    "@types/node": "^20.12.12",
    "globals": "^15.3.0",
    "tsx": "^4.10.5",
    "typescript-eslint": "^7.10.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "discord.js": "^14.15.2",
    "eslint": "9.x"
  }
}